/*Beginning of smth huge(or not)*/

#include <iostream>

int main()
{
    using std::cout;
    using std::cin;
    using std::endl;
    
    int num1;
    int num2;
    int sum;

    cout << "Please enter two nymbers separated by a space:" <<endl;
    cin>>num1>>num2;

    sum = num1 + num2;

    cout<<num1<<" + "<<num2<<"= "<<sum<<endl;

    return 0;
}

